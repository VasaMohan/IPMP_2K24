#include<bits/stdc++.h>
using namespace std;

int getoddoccurrance(int arr[], int arr_size)
{
    for(int i=0;i<arr_size;i++)
    {
       int count=0;
    
    for(int j=0;j<arr_size;j++)
    {
        if(arr[i] == arr[j]) 
        count++;
    }
        if(count %2 != 0)
        return arr[i];
        
    }    
    return -1;
}

int main()
{   
    int arr[] = {5,6,2,6,3,6,1,2,3};
    
    int n = sizeof(arr)/ sizeof(arr[0]);
    
    cout<<getoddoccurrance(arr,n);
    return 0;
}
